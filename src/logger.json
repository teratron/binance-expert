{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "simple": {
      "format": "%(asctime)s %(name)s: %(levelname)s: %(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "DEBUG",
      "formatter": "simple",
      "stream": "ext://sys.stdout"
    },
    "console_error": {
      "class": "logging.StreamHandler",
      "level": "ERROR",
      "formatter": "simple",
      "stream": "ext://sys.stderr"
    },
    "file": {
      "class": "logging.FileHandler",
      "level": "WARNING",
      "formatter": "simple",
      "filename": "data.log",
      "mode": "a",
      "encoding": "utf-8"
    }
  },
  "loggers": {
    "example": {
      "level": "INFO",
      "handlers": [
        "console_error"
      ],
      "propagate": "no"
    }
  },
  "root": {
    "level": "DEBUG",
    "handlers": [
      "console",
      "console_error",
      "file"
    ]
  }
}